<view class="menus_group item_box_shadow">
    <view class="menu_item" wx:for="{{list}}" wx:key="index" data-path="{{item.path}}" bindtap="jump">
        <image class="menu_icon" src="{{item.icon}}"></image>
        <view class="menu_main">
            <view class="menu_name">{{item.name}}</view>
            <image class="menu_arrow" src="/assets/static/images/icon/all_right.png"></image>
        </view>
    </view>
</view>